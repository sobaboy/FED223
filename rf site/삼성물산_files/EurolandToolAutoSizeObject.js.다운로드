var EurolandToolAutoSizeObject=new function(){this.marginOffset=0;var o=this,r=0,e=null,i=0,f=s(),n=false,a=false,t=null,l=[],u=false;function s(){if(document.all){return typeof document.documentMode=="undefined"?d():Math.floor(parseFloat(document.documentMode))}return 0}function d(){if(!document.all)return 0;if(f!=null)return f;var e,t=navigator.userAgent;e=t.search("MSIE");if(e==-1)return 0;f=t.substring(e+4,t.indexOf(";",e));f=Math.floor(parseFloat(f));return f}function c(r,e){var t,n=(r.ownerDocument||document).defaultView;if(n&&n.getComputedStyle){e=e.replace(/([A-Z])/g,"-$1").toLowerCase();return n.getComputedStyle(r,null).getPropertyValue(e)}else if(r.currentStyle){e=e.replace(/\-(\w)/g,function(e,t){return t.toUpperCase()});t=r.currentStyle[e];if(/^\d+(em|pt|%|ex)?$/i.test(t)){return function(e){var t=r.style.left,n=r.runtimeStyle.left;r.runtimeStyle.left=r.currentStyle.left;r.style.left=e||0;e=r.style.pixelLeft+"px";r.style.left=t;r.runtimeStyle.left=n;return e}(t)}return t}}function p(){if(!window.postMessage||f&&f<8||window==window.top){return}if(window.addEventListener){window.addEventListener("message",w)}else{window.attachEvent("onmessage",w)}}function w(e){if(!e)e=window.event;if(typeof e.data!="string")return;var t=e.data.split("-");if(t[0]!="ActivateEurolandToolAutoSizeObject")return;if(t.length==2||t.length==3){try{i=parseFloat(t[1]);n=true}catch(e){return}if(t.length==3&&parseFloat(t[2])){u=true}if(window.removeEventListener){window.removeEventListener("message",w)}else{window.detachEvent("onmessage",w)}g()}}function g(){if(t!=null||!a||!n)return;t=setInterval(function(){h()},30)}function m(e){var t=e.indexOf("#");if(t>-1){e=e.substring(0,t)}return e}function h(){var e=0,t,n;t=l.length;while(t--){n=parseFloat(l[t].offsetHeight)+o.marginOffset;if(n>e){e=n}}if(e!=r){r=e;window[u?"parent":"top"].postMessage('{ "iFrame" : { "height" : '+r+', "index" : '+i+" }}","*")}}this.load=function(e){if(!window.postMessage||f&&f<8||window==window.top){return}var t,n,r,i;if(!o.marginOffset){try{o.marginOffset=parseFloat(c(document.body,"margin-top").replace("px",""))+parseFloat(c(document.body,"margin-bottom").replace("px",""))+20;if(isNaN(o.marginOffset)){o.marginOffset=20}}catch(e){o.marginOffset=20}}if(typeof e=="string"||typeof e=="object"&&!("length"in e)){r=[];r.push(e)}else{r=e}n=r.length;while(n--){t=r[n];if(typeof t=="string"){t=document.getElementById(t)}if(typeof t!="object"||t==null||window.parent==window)continue;l.push(t)}if(!l.length){return false}a=true;g();return true};p()};